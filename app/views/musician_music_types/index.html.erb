<h1>Listing musician_music_types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musician_music_types.each do |musician_music_type| %>
      <tr>
        <td><%= link_to 'Show', musician_music_type %></td>
        <td><%= link_to 'Edit', edit_musician_music_type_path(musician_music_type) %></td>
